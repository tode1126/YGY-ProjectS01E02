<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org// DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="spring.data.ReboardDto" alias="bdto"/>
		<typeAlias type="spring.data.noticeDto" alias="ndto"/>
		<typeAlias type="spring.data.QnADto" alias="qdto"/>
		<typeAlias type="spring.data.UserDto" alias="userDto"/>
		<typeAlias type="spring.data.UserSearchDto" alias="userSearchDto"/>
		<typeAlias type="spring.data.restaurant.RestaurantDto" alias="rdto"/>
		<typeAlias type="spring.data.restaurant.RestaurantMenuDto" alias="rmdto"/>
		<typeAlias type="spring.data.restaurant.RestaurantTableDto" alias="rtdto"/>
		<typeAlias type="spring.data.restaurant.RestaurantIntroDto" alias="ridto"/>
		<typeAlias type="spring.data.restaurant.RestaurantIntroImageDto" alias="riimgdto"/>
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/sql/ReboardSql.xml"/>
		<mapper resource="mybatis/sql/noticesql.xml"/>
		<mapper resource="mybatis/sql/QnAsql.xml"/>
		<mapper resource="mybatis/sql/UserSql.xml"/>
		<mapper resource="mybatis/sql/AdminSql.xml"/>
		<mapper resource="mybatis/sql/RestaurantSql.xml"/>
		<mapper resource="mybatis/sql/RestaurantMenuSql.xml"/>
		<mapper resource="mybatis/sql/RestaurantIntroSql.xml"/>
	</mappers>
</configuration>